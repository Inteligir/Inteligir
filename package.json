{
  "name": "inteligir",
  "description": "Share knowledge from the web through playlists",
  "author": "Kaiyu Hsu <im.kaiyu@gmail.com> (http://github.com/tehkaiyu)",
  "license": "MIT",
  "version": "0.0.0",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/its-bananas/inteligir"
  },
  "engines": {
    "node": "10.15.3",
    "npm": "6.9.0"
  },
  "scripts": {
    "setup": "npm i && next run build && cp ./config/.env.example ./config/.env.local && npm run prisma:generate",
    "build": "next build client",
    "start": "node --max_old_space_size=460 server",
    "dev": "",
    "dev:prisma": "prisma2 dev",
    "dev:server": "env-cmd -f ./config/.env.local nodemon server/index.js --ext js,graphql",
    "dev:storybook": "start-storybook -p 5010 -s ./client",
    "prisma:generate": "env-cmd -f ./config/.env.local prisma2 generate",
    "deploy": "node tools/deploy",
    "deploy:storybook": "storybook-to-ghpages",
    "lint": "eslint --ext .js --env browser,node client/ server/",
    "lint:fix": "eslint --fix --ext .js --env browser,node client/ server",
    "create:components": "node ./tools/components/create-components.js",
    "test": "echo \"Error: no test specified\" && exit 0",
    "heroku-postbuild": "prisma2 generate"
  },
  "_moduleAliases": {
    "@server": "./server",
    "@client": "./client",
    "@components": "./client/components"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.5.0",
    "adorable-avatars": "^0.4.1",
    "apollo-boost": "^0.4.4",
    "apollo-server": "^2.9.6",
    "apollo-server-express": "^2.9.6",
    "bcrypt": "^3.0.6",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-import": "^0.7.1",
    "graphql-middleware": "^4.0.1",
    "graphql-shield": "^6.1.0",
    "graphql-tag": "^2.10.1",
    "helmet": "^3.21.1",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "moniker": "^0.1.2",
    "next": "^9.1.1",
    "next-offline": "^4.0.6",
    "nexus": "0.12.0-beta.14",
    "nexus-prisma": "^0.5.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-content-loader": "^4.3.2",
    "react-dom": "^16.10.2",
    "react-swipeable-views": "^0.13.3",
    "react-transition-group": "^4.3.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@sambego/storybook-state": "^1.3.6",
    "@storybook/addon-a11y": "^5.2.4",
    "@storybook/addon-actions": "^5.2.4",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-knobs": "^5.2.4",
    "@storybook/addon-links": "^5.2.4",
    "@storybook/addon-notes": "^5.2.4",
    "@storybook/addon-storysource": "^5.2.4",
    "@storybook/addon-viewport": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "bluebird": "^3.7.1",
    "env-cmd": "^10.0.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.0",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "prisma2": "^2.0.0-preview014.2",
    "react-test-renderer": "^16.10.2"
  }
}
