{
  "name": "Inteligir",
  "description": "Making open source accessible for everyone",
  "author": "Kaiyu Hsu <im.kaiyu@gmail.com> (http://github.com/tehkaiyu)",
  "license": "MIT",
  "version": "0.0.0",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/inteligir/inteligir-platform"
  },
  "engines": {
    "node": "10.14.2",
    "npm": "6.4.1"
  },
  "scripts": {
    "setup": "npm i && node tools/generate_env",
    "dev": "env-cmd ./config/local.env nodemon server/index.js --ext js,graphql",
    "build": "next build client",
    "deploy": "env-cmd ./config/local.env node tools/deploy",
    "get-schema": "graphql get-schema -p prisma --dotenv config/local.env",
    "analyze": "source-map-explorer client/build/static/js/main.*",
    "test": "env-cmd ./config/test.env jest --watch --runInBand",
    "heroku-postbuild": "npm run build"
  },
  "_moduleAliases": {
    "@root": ".",
    "@server": "./server",
    "@client": "./client"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./tests/setup.js"
  },
  "dependencies": {
    "apollo-boost": "0.1.16",
    "apollo-client": "2.4.2",
    "adorable-avatars": "0.2.1",
    "aws-sdk": "2.380.0",
    "body-parser": "1.18.3",
    "boom": "7.3.0",
    "cookie-session": "2.0.0-beta.3",
    "evergreen-ui": "4.8.0",
    "express": "4.16.4",
    "express-async-errors": "3.1.1",
    "helmet": "3.15.0",
    "lodash": "4.17.11",
    "module-alias": "2.1.0",
    "next": "7.0.2",
    "nprogress": "0.2.0",
    "passport": "0.4.0",
    "passport-google-oauth20": "1.0.0",
    "polished": "2.3.1",
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "redis": "2.8.0",
    "styled-components": "4.1.3",
    "uuid": "3.3.2",
    "graphql": "14.0.2",
    "graphql-yoga": "1.14.10",
    "react-adopt": "0.6.0",
    "react-apollo": "2.2.1",
    "prisma-binding": "2.1.1",
    "subscriptions-transport-ws": "0.9.15",
    "react-transition-group": "2.5.0",
    "workbox-webpack-plugin": "3.6.3"
  },
  "devDependencies": {
    "graphql-cli": "2.16.4",
    "babel-eslint": "10.0.1",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-styled-components": "1.10.0",
    "dotenv": "6.2.0",
    "eslint": "5.10.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "foreman": "3.0.1",
    "husky": "1.2.1",
    "inquirer": "6.2.1",
    "jest": "23.6.0",
    "nodemon": "1.18.8",
    "prettier": "1.15.3",
    "pretty-quick": "1.8.0",
    "puppeteer": "1.11.0",
    "source-map-explorer": "1.6.0"
  }
}
