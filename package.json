{
  "name": "inteligir",
  "description": "Share knowledge from the web through playlists",
  "author": "Kaiyu Hsu <im.kaiyu@gmail.com> (http://github.com/tehkaiyu)",
  "license": "MIT",
  "version": "0.0.0",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/its-bananas/inteligir"
  },
  "engines": {
    "node": "10.15.3",
    "npm": "6.9.0"
  },
  "scripts": {
    "setup": "npm i && env-cmd -e local npm run heroku-postbuild",
    "build": "next build client",
    "start": "node server",
    "heroku-postbuild": "prisma2 generate && npm run build",
    "dev": "concurrently \"npm run dev:prisma\" \"npm run dev:server\"",
    "dev:prisma": "env-cmd -e local prisma2 dev",
    "dev:server": "env-cmd -e local nodemon server/index.js",
    "dev:storybook": "env-cmd -e local start-storybook -p 5010 -s ./client",
    "generate:prisma": "env-cmd -e local prisma2 generate",
    "generate:seed": "env-cmd -e local node ./prisma/seed.js",
    "generate:components": "node ./tools/components/create-components.js",
    "deploy": "node tools/deploy",
    "deploy:storybook": "storybook-to-ghpages",
    "lint": "eslint --ext .js --env browser,node client/ server/",
    "lint:fix": "eslint --fix --ext .js --env browser,node client/ server",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "_moduleAliases": {
    "@server": "./server",
    "@client": "./client",
    "@components": "./client/components",
    "@containers": "./client/containers",
    "@hooks": "./client/hooks",
    "@utils": "./client/utils"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.6.0",
    "@prisma/sdk": "^0.0.30",
    "adorable-avatars": "^0.4.1",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "apollo-server": "^2.9.7",
    "apollo-server-express": "^2.9.7",
    "apollo-utilities": "^1.3.2",
    "bcrypt": "^3.0.6",
    "classnames": "^2.2.6",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.0.2",
    "graphql-tag": "^2.10.1",
    "helmet": "^3.21.2",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "moniker": "^0.1.2",
    "next": "^9.1.3",
    "next-offline": "^4.0.6",
    "nexus": "0.12.0-rc.4",
    "nexus-prisma": "^0.5.2",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-content-loader": "^4.3.2",
    "react-dom": "^16.11.0",
    "react-hook-form": "^3.27.0",
    "react-swipeable-views": "^0.13.3",
    "react-transition-group": "^4.3.0"
  },
  "devDependencies": {
    "@sambego/storybook-state": "^1.3.6",
    "@storybook/addon-a11y": "^5.2.6",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addon-notes": "^5.2.6",
    "@storybook/addon-storysource": "^5.2.6",
    "@storybook/addon-viewport": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "bluebird": "^3.7.1",
    "concurrently": "^5.0.0",
    "env-cmd": "^10.0.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.0",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "prisma2": "^2.0.0-preview016.2",
    "react-test-renderer": "^16.11.0"
  }
}
