{
  "name": "inteligir",
  "description": "Share knowledge from the web through playlists",
  "author": "Kaiyu Hsu <im.kaiyu@gmail.com> (http://github.com/tehkaiyu)",
  "license": "MIT",
  "version": "0.0.0",
  "main": "dist/server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/its-bananas/inteligir"
  },
  "engines": {
    "node": "10.15.3",
    "npm": "6.9.0"
  },
  "scripts": {
    "setup": "npm i && npm run heroku-postbuild",
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "heroku-postbuild": "npm run db:migrate && npm run generate npm run build",
    "build": "env-cmd npm run clean && next build && tsc --project tsconfig.server.json",
    "dev": "concurrently \"npm run dev:prisma\" \"npm run dev:server\"",
    "dev:prisma": "env-cmd prisma2 generate --watch",
    "dev:server": "env-cmd ts-node-dev --no-notify --respawn --transpileOnly --project tsconfig.server.json -r tsconfig-paths/register server",
    "dev:db": "env-cmd prisma2 studio --experimental",
    "generate": "npm run generate:prisma && npm run generate:nexus && npm run generate:codegen",
    "generate:prisma": "env-cmd prisma2 generate",
    "generate:nexus": "env-cmd ts-node --transpile-only --project tsconfig.server.json -r tsconfig-paths/register server/schema",
    "generate:codegen": "env-cmd graphql-let",
    "db:seed": "env-cmd ts-node prisma/seed",
    "db:introspect": "env-cmd prisma2 introspect",
    "db:migrate": "env-cmd prisma2 migrate --experimental",
    "deploy": "node tools/deploy",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@apollo/react-common": "^3.1.3",
    "@apollo/react-components": "^3.1.3",
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.0",
    "@prisma/client": "^2.0.0-preview021",
    "adorable-avatars": "^0.4.1",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "apollo-link-schema": "^1.2.4",
    "apollo-server": "^2.9.16",
    "apollo-server-express": "^2.9.16",
    "apollo-utilities": "^1.3.3",
    "bcrypt": "^3.0.7",
    "classnames": "^2.2.6",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.0.9",
    "graphql-tag": "^2.10.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "next": "^9.2.1",
    "next-offline": "^5.0.0",
    "nexus": "0.12.0-rc.9",
    "nexus-prisma": "^0.9.0",
    "nprogress": "^0.2.0",
    "react": "^16.12.0",
    "react-content-loader": "^5.0.1",
    "react-dom": "^16.12.0",
    "react-hook-form": "^4.8.0",
    "react-swipeable-views": "^0.13.7",
    "react-transition-group": "^4.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/typescript": "^1.12.1",
    "@graphql-codegen/typescript-operations": "^1.12.1",
    "@graphql-codegen/typescript-react-apollo": "^1.12.1",
    "@prisma/sdk": "^0.0.182",
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/classnames": "^2.2.9",
    "@types/cookie": "^0.3.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/helmet": "^0.0.45",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/node": "^13.7.0",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/webpack": "^4.41.4",
    "@types/ws": "^7.2.1",
    "concurrently": "^5.1.0",
    "env-cmd": "^10.0.1",
    "fs-extra": "^8.1.0",
    "graphql-let": "^0.6.2",
    "husky": "^4.2.1",
    "inquirer": "^7.0.4",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "prisma2": "^2.0.0-preview021",
    "react-test-renderer": "^16.12.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.5"
  }
}
