{
  "name": "inteligir",
  "description": "Share knowledge from the web through playlists",
  "author": "Kaiyu Hsu <im.kaiyu@gmail.com> (http://github.com/tehkaiyu)",
  "license": "MIT",
  "version": "0.0.0",
  "main": "server/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/its-bananas/inteligir"
  },
  "engines": {
    "node": "10.15.3",
    "npm": "6.9.0"
  },
  "scripts": {
    "setup": "npm i && npm run heroku-postbuild",
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "heroku-postbuild": "npm run migrate:up up && npm run build",
    "build": "npm -s run clean && npm -s run generate && tsc && next build client",
    "dev": "concurrently \"npm run dev:prisma\" \"npm run dev:server\"",
    "dev:prisma": "prisma2 dev",
    "dev:server": "ts-node-dev --no-notify --respawn --transpileOnly server",
    "dev:storybook": "start-storybook -p 5010 -s ./client",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma2 generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "generate:seed": "ts-node prisma/seed",
    "migrate:save": "prisma2 lift save",
    "migrate:up": "prisma2 lift up",
    "deploy": "node tools/deploy",
    "deploy:storybook": "storybook-to-ghpages",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "_moduleAliases": {
    "@server": "dist/server",
    "@client": "dist/client",
    "@components": "dist/client/components",
    "@containers": "dist/client/containers",
    "@hooks": "dist/client/hooks",
    "@utils": "dist/client/utils",
    "@storyUtils": "dist/.storybook/utils.js"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "arrowParens": "always",
    "trailingComma": "all"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.8.2",
    "@prisma/photon": "^2.0.0-preview019",
    "adorable-avatars": "^0.4.1",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "apollo-server": "^2.9.16",
    "apollo-server-express": "^2.9.16",
    "apollo-utilities": "^1.3.3",
    "bcrypt": "^3.0.7",
    "classnames": "^2.2.6",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.0.7",
    "graphql-tag": "^2.10.1",
    "helmet": "^3.21.2",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "module-alias": "^2.2.2",
    "next": "^9.1.7",
    "next-offline": "^4.0.6",
    "nexus": "0.12.0-rc.5",
    "nexus-prisma": "^0.6.1",
    "nprogress": "^0.2.0",
    "react": "^16.12.0",
    "react-content-loader": "^4.3.4",
    "react-dom": "^16.12.0",
    "react-hook-form": "^4.4.8",
    "react-swipeable-views": "^0.13.3",
    "react-transition-group": "^4.3.0"
  },
  "devDependencies": {
    "@types/next": "9.0.0",
    "@types/bcryptjs": "2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/helmet": "0.0.45",
    "@types/jsonwebtoken": "8.3.5",
    "@types/ws": "6.0.4",
    "@prisma/sdk": "^0.0.126",
    "@types/node": "12.12.24",
    "ts-node": "8.6.0",
    "ts-node-dev": "1.0.0-pre.44",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "typescript": "3.7.4",
    "@sambego/storybook-state": "^1.3.6",
    "@storybook/addon-a11y": "^5.2.8",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-docs": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addon-storysource": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@storybook/source-loader": "^5.2.8",
    "@storybook/storybook-deployer": "^2.8.1",
    "@storybook/theming": "^5.2.8",
    "concurrently": "^5.0.2",
    "env-cmd": "^10.0.1",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.3",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "prisma2": "^2.0.0-preview019",
    "react-test-renderer": "^16.12.0"
  }
}
